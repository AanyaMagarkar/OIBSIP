import java.util.ArrayList;
import java.util.Iterator;
import java.util.List;

class Library {
    private List<Book> books = new ArrayList();
    private List<Magazine> magazines = new ArrayList();
    private int totalBooksSold;
    private int totalMagazinesSold;

    public Library() {
    }

    public void addBook(String title) {
        Book book = new Book(title);
        this.books.add(book);
    }

    public void addMagazine(String title) {
        Magazine magazine = new Magazine(title);
        this.magazines.add(magazine);
    }

    public void orderBook() {
        System.out.println("Ordering a book");
        this.incrementBookSales();
    }

    public void orderMagazine() {
        System.out.println("Ordering a magazine");
        this.incrementMagazineSales();
    }

    public void displayBooks() {
        System.out.println("Existing Books:");
        Iterator var1 = this.books.iterator();

        while(var1.hasNext()) {
            Book book = (Book)var1.next();
            System.out.println(book.getTitle());
        }

    }

    public void displayMagazines() {
        System.out.println("Existing Magazines:");
        Iterator var1 = this.magazines.iterator();

        while(var1.hasNext()) {
            Magazine magazine = (Magazine)var1.next();
            System.out.println(magazine.getTitle());
        }

    }

    public void incrementBookSales() {
        ++this.totalBooksSold;
    }

    public void incrementMagazineSales() {
        ++this.totalMagazinesSold;
    }

    public int getTotalBooksSold() {
        return this.totalBooksSold;
    }

    public int getTotalMagazinesSold() {
        return this.totalMagazinesSold;
    }
}
